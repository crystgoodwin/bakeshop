               "name": "Bakeshop Gray 100"
            },
            {
               "slug": "b-gray-200",
               "color": "rgb(229 231 235)",
               "name": "Bakeshop Gray 200"
            },
            {
               "slug": "b-gray-300",
               "color": "rgb(209 213 219)",
               "name": "Bakeshop Gray 300"
            },
            {
               "slug": "b-gray-400",
               "color": "rgb(156 163 175)",
               "name": "Bakeshop Gray 400"
            },
            {
               "slug": "b-gray-500",
               "color": "rgb(107 114 128)",
               "name": "Bakeshop Gray 500"
            },
            {
               "slug": "b-gray-600",
               "color": "rgb(75 85 99)",
               "name": "Bakeshop Gray 600"
            },
            {
               "slug": "b-gray-700",
               "color": "rgb(55 65 81)",
               "name": "Bakeshop Gray 700"
            },
            {
               "slug": "b-gray-800",
               "color": "rgb(31 41 55)",
               "name": "Bakeshop Gray 800"
            },
            {
               "slug": "b-gray-900",
               "color": "rgb(17 24 39)",
               "name": "Bakeshop Gray 900"
            },
            {
               "slug": "b-primary",
               "color": "rgb(239 68 68)",
               "name": "Bakeshop Primary"
            },
            {
               "slug": "b-accent",
               "color": "rgb(67 56 202)",
               "name": "Bakeshop Accent"
            },
            {
               "slug": "b-success",
               "color": "rgb(52 211 153)",
               "name": "Bakeshop Success"
            },
            {
               "slug": "b-info",
               "color": "rgb(96 165 250)",
               "name": "Bakeshop Info"
            },
            {
               "slug": "b-warning",
               "color": "rgb(251 146 60)",
               "name": "Bakeshop Warning"
            },
            {
               "slug": "b-danger",
               "color": "rgb(236 72 153)",
               "name": "Bakeshop Danger"
            }
         ],
         "background": true,
         "text": true,
         "link": true,
         "custom": true,
         "duotone": [
            {
               "slug": "b-pink-sunset",
               "colors": [
                  "#11245e",
                  "#dc4379"
               ],
               "name": "Bakeshop Pink Sunset"
            }
         ],
         "defaultGradients": false,
         "gradients": [
            {
               "slug": "b-summer-haze",
               "name": "Bakeshop Summer Haze",
               "gradient": "linear-gradient(#a8ff78, #78ffd6)"
            }
         ]
      },
      "blocks": {
         "core/site-title": {
            "color": {
               "defaultPalette": true,
               "palette": [
                  {
                     "slug": "b-primary",
                     "color": "#ef4444",
                     "name": "Bakeshop Primary"
                  }
               ],
               "link": true,
               "custom": false,
               "gradients": [],
               "customGradient": false
            },
            "typography": {
               "fontFamilies": [
                  {
                     "fontFamily": "Pacifico, sans-serif",
                     "slug": "b-pacifico",
                     "name": "Bakeshop Pacifico"
                  }
               ]
            }
         },
         "core/cover": {
            "color": {
               "duotone": [],
               "customDuotone": false
            }
         },
         "core/preformatted": {
            "typography": {
               "fontSizes": [],
               "customFontSize": false
            }
         }
      },
      "border": {
         "color": true,
         "radius": true,
         "style": true,
         "width": true
      },
      "typography": {
         "fontFamilies": [
            {
               "fontFamily": "Rubik, sans-serif",
               "slug": "b-rubik",
               "name": "Bakeshop Rubik"
            }
         ],
         "fontSizes": [
            {
               "slug": "small",
               "size": "0.75rem",
               "name": "Small"
            },
            {
               "slug": "medium",
               "size": "1.25rem",
               "name": "Medium"
            },
            {
               "slug": "large",
               "size": "2.25rem",
               "name": "Large"
            },
            {
               "slug": "x-large",
               "size": "3rem",
               "name": "Extra Large"
            },
            {
               "slug": "gigantic",
               "size": "3.75rem",
               "name": "Gigantic"
            }
         ],
         "lineHeight": true,
         "dropCap": true,
         "fontWeight": true,
         "fontStyle": true,
         "textTransform": true,
         "letterSpacing": true,
         "textDecoration": true
      },
      "layout": {
         "contentSize": "800px",
         "wideSize": "1100px"
      },
      "spacing": {
         "margin": true,
         "padding": true,
         "units": [
            "px",
            "rem",
            "vh",
            "vw",
            "%"
         ],
         "blockGap": true
      }
   },
   "styles": {
      "color": {
         "text": "var(--wp--preset--color--b-gray-700)",
         "background": "var(--wp--preset--color--b-white)"
      },
      "elements": {
         "link": {
            "color": {
               "text": "var(--wp--preset--color--b-primary)"
            }
         }
      },
      "blocks": {
         "core/button": {
            "color": {
               "gradient": "var(--wp--preset--gradient--b-summer-haze)"
            }
         },
         "core/pullquote": {
            "border": {
               "width": "2px",
               "radius": "10px",
               "style": "solid",
               "color": "var(--wp--preset--color--b-primary)"
            }
         },
         "core/site-title": {
            "typography": {
               "fontFamily": "var(--wp--preset--font-family--b-pacifico)"
            }
         }
      },
      "typography": {
         "fontFamily": "var(--wp--preset--font--family--b-rubrik)",
         "fontSize": "16px",
         "fontStyle": "normal",
         "fontWeight": "normal",
         "lineHeight": "inherit",
         "textDecoration": "none",
         "textTransform": "none"
      },
      "spacing": {
         "margin": {
            "top": "0px",
            "bottom": "0px",
            "left": "0px",
            "right": "0px"
         },
         "padding": {
            "top": "0px",
            "bottom": "0px",
            "left": "0px",
            "right": "0px"
         },
         "blockGap": "4rem"
      }
   }
}